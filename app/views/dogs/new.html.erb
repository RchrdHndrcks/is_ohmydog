<!-- app/views/dogs/new.html.erb -->
<%= form_for @dog, html: { multipart: true } do |f| %>
  <label> Nombre </label>
  <%= f.text_field :name %>

  <label> Raza </label>
  <%= f.text_field :breed %>

  <label> Fecha de Nacimiento </label>
  <%= f.date_field :birthdate %>
  
  <label> Foto </label>
  <%= f.file_field :image %>

  <%= f.hidden_field :user_id, value: params[:user_id] %>

  <%= f.submit 'Registrar Perro' %>
<% end %>